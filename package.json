{
  "name": "@imcyee/superstream",
  "version": "0.0.5",
  "description": "stream for node",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/imcyee/superstream#README",
  "repository": {
    "type": "git",
    "url": "https://github.com/imcyee/superstream.git"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "start:dev": "ts-node-dev ./src/index.ts",
    "start:server": "ts-node ./server/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch:debug": "DEBUG=test:* jest --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/statsd-client": "^0.4.3",
    "cassandra-driver": "^4.6.3",
    "dayjs": "^1.10.7",
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "redis": "^4.0.0-rc.2",
    "statsd-client": "^0.4.7",
    "supports-color": "^9.0.2",
    "testcontainers": "^7.20.4",
    "ts-jest": "^27.0.5",
    "ts-mixer": "^6.0.0",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.170",
    "jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
