{
  "name": "stream-node",
  "version": "1.0.0",
  "description": "stream for node",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start:dev": "ts-node-dev ./src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cassandra-driver": "^4.6.3",
    "dayjs": "^1.10.7",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "lodash": "^4.17.21",
    "redis": "^4.0.0-rc.2",
    "testcontainers": "^7.20.4",
    "ts-jest": "^27.0.5",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/async-redis": "^1.1.2",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.170",
    "jest": "^27.2.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
